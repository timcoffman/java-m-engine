{
TAG: DoLoop
CMD: QUIT | (-)

TAG: loopBody( i )
CMD: QUIT | (-)

TAG: sometimes
CMD: QUIT | 1

TAG: doOnceLoop
CMD: FOR | (-) {
  CMD: DO | loopBody^DoLoop/LOCAL( -1 )
  CMD: QUIT | (-)
}
CMD: QUIT | (-)

TAG: doForeverLoop
CMD: FOR | (-) {
  CMD: DO | loopBody^DoLoop/LOCAL( -1 )
  CMD: QUIT : $$sometimes^DoLoop/LOCAL(  ) | (-)
}
CMD: QUIT | (-)

TAG: doWhileLoop
CMD: NEW | i
CMD: SET | i := 0
CMD: FOR | (-) {
  CMD: DO | loopBody^DoLoop/LOCAL( i )
  CMD: SET | i := i-1
  CMD: QUIT : '(i<10) | (-)
}
CMD: QUIT | (-)

}