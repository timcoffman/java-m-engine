{
TAG: WhileLoop
CMD: QUIT | (-)

TAG: loopBody( i )
CMD: QUIT | (-)

TAG: forLoopIncrementByOne
CMD: NEW | i
CMD: FOR | i := 1:1:10 {
  CMD: DO | loopBody^WhileLoop/LOCAL( i )
}
CMD: QUIT | (-)

TAG: forLoopIncrementByTwo
CMD: NEW | i
CMD: FOR | i := 1:2:10 {
  CMD: DO | loopBody^WhileLoop/LOCAL( i )
}
CMD: QUIT | (-)

TAG: forLoopIncrementByOneForever
CMD: NEW | i
CMD: FOR | i := 1 {
  CMD: DO | loopBody^WhileLoop/LOCAL( i )
}
CMD: QUIT | (-)

TAG: forLoopIncrementByTwoForever
CMD: NEW | i
CMD: FOR | i := 1:2 {
  CMD: DO | loopBody^WhileLoop/LOCAL( i )
}
CMD: QUIT | (-)

TAG: forLoopStopNotIncludedIncrementByOne
CMD: NEW | i
CMD: SET | i := 0
CMD: FOR | (-) {
  CMD: QUIT : '(i<10) | (-)
  CMD: DO | (-)
  {
    CMD: DO | loopBody^WhileLoop/LOCAL( i )
    CMD: SET | i := i+1
  }
}
CMD: QUIT | (-)

TAG: forLoopStopNotIncludedIncrementByTwo
CMD: NEW | i
CMD: SET | i := 0
CMD: FOR | (-) {
  CMD: QUIT : '(i<10) | (-)
  CMD: DO | (-)
  {
    CMD: DO | loopBody^WhileLoop/LOCAL( i )
    CMD: SET | i := i+2
  }
}
CMD: QUIT | (-)

}